
<div id="headline" class='jumbotron MintHeadline'>
    <div class='container'>
        <h1 id="headline-h"><%=Data['mainTitle']%></h1>
        <p id="headline-p"><%=Data['subTitle']%></p>
    </div>
</div>

<div id="paragraph" class="MintParagraph">
    <div class="jumbotron">
        <p id="paragraph-p" class="lead"><%=Data['paraContent']%></p>
    </div>
</div>

<div id="article" class="MintArticle">
    <div class="container">
        <div class="jumbotron">
            <h1><%=Data['articleTitle']%></h1>
            <hr class='featurette-divider' />
            <p><%=Data['articleContent']%></p>
        </div>
    </div>
</div>

<div id="single-bottom" class="MintSingle">
    <div class="jumbotron">
        <div class="SingleHolder">
            <img src="<%=Data['singleLink']%>" alt="..." class="img-thumbnail">
            <br />
            <p><%=Data['singleContent']%></p>
        </div>
    </div>
</div>

<div id="single-top" class="MintSingle">
    <div class="jumbotron">
        <div class="SingleHolder">
            <p><%=Data['singleContent']%></p>
            <img src="<%=Data['singleLink']%>" alt="..." class="img-thumbnail">
        </div>
    </div>
</div>

<div id="single-left" class="MintSingle">
    <div class="jumbotron">
        <div class="row SingleHolder">
            <div class="col-lg-5">
                <p><%=Data['singleContent']%></p>
            </div>
            <div class="col-lg-7">
                <img src="<%=Data['singleLink']%>" alt="..." class="img-thumbnail">
            </div>
        </div>
    </div>
</div>

<div id="single-right" class="MintSingle">
    <div class="jumbotron">
        <div class="row SingleHolder">
            <div class="col-lg-7">
                <img src="<%=Data['singleLink']%>" alt="..." class="img-thumbnail">
            </div>
            <div class="col-lg-5">
                <p><%=Data['singleContent']%></p>
            </div>
        </div>
    </div>
</div>

<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
        <li data-target="#carousel-example-generic" data-slide-to="1"></li>
        <li data-target="#carousel-example-generic" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="<%= Data['galleryLink1']%>" alt="...">
            <div class="carousel-caption">
            </div>
        </div>
        <div class="item">
            <img src="<%= Data['galleryLink2']%>" alt="...">
            <div class="carousel-caption">
            </div>
        </div>
        <div class="item">
            <img src="<%= Data['galleryLink3']%>" alt="...">
            <div class="carousel-caption">
            </div>
        </div>
    </div>
    <!-- Controls -->
    <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<div id="video" class="MintVideo">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <video class="video-js" preload="auto" poster="images/aya.jpg" data-setup="{}">
                    <!--<source src="http://iurevych.github.com/Flat-UI-videos/big_buck_bunny.mp4" type="video/mp4">-->
                    <!--<source src="http://iurevych.github.com/Flat-UI-videos/big_buck_bunny.webm" type="video/webm">-->
                </video>
            </div>
        </div>
    </div>
</div>